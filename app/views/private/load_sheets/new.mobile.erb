<%= form_for [:private, @load_sheet] do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <br />
  <hr />
  <%= f.submit "Create Load Sheet" %>,
  <%= f.submit "Create and Add Another Load Sheet", :name => 'submit_and_create_another_load_sheet' %>
  or
  <%= link_to 'Cancel', private_home_path %>
<% end %>
